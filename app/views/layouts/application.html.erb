<!DOCTYPE html>
<html>
  <head>
    <title>Conterest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href='https://fonts.googleapis.com/css?family=Black+Ops+One|Cabin' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
        window.logoURL = "<%= image_url('logo.png') %>";
        window.editURL = "<%= image_url('pencil.jpeg') %>";
    </script>
    <%= favicon_link_tag asset_path('logo.png') %>
    <script
      src="https://use.fontawesome.com/releases/v5.13.0/js/all.js"
      data-auto-a11y="true"
    ></script>

  </head>
  <body>
    <%= yield %>
  </body>
</html>
