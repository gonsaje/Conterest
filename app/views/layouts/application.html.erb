<!DOCTYPE html>
<html>
  <head>
    <title>Conterest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href='https://fonts.googleapis.com/css?family=Nova+Cut|Black+Ops+One|Barlow+Condensed|Cabin' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script src=“https://use.fontawesome.com/releases/v5.13.0/js/all.js” data-auto-a11y=“true”></script>
    <script type="text/javascript">
      window.belowWater = "<%= image_url('below_water.jpeg') %>";
      window.highRise = "<%= image_url('high-rise.jpg') %>";
      window.menu = "<%= image_url('hamburger-red-menu.png') %>";
      window.menuLogout = "<%= image_url('menu-logout.png') %>";
    </script>
     
  </head>
  <body>
    <%= yield %>
  </body>
</html>
